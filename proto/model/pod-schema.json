                    "pattern": "^mailto:[\\w\\_\\~\\!\\$\\&\\'\\(\\)\\*\\+\\,\\;\\=\\:.-]+@[\\w.-]+\\.[\\w.-]+?$",
                    "asOntology": {
                        "@context": "profile-schema-onto.json",
                        "prefLabel": "Contact Email",
                        "referenceProperty": "vcard:hasEmail"
                    }
                }
            },
            "required": [ "fn", "hasEmail" ],
            "asOntology": {
                "@context": "profile-schema-onto.json",
