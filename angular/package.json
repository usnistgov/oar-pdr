    "@types/express": "^4.15.0",
    "@types/file-saver": "^2.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.0.30",
    "@types/webpack-env": "^1.13.0",
    "angular-mocks": "^1.6.10",
    "ng-mock-e2e": "^0.1.4",
    "ajv": "^6.12.3",
    "codelyzer": "^4.0.2",
    "http-server": "^0.10.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
